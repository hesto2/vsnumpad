{
    "name": "vsnumpad",
    "displayName": "VsNumpad",
    "description": "A simple extension to remap letter keys+modifiers to a numpad. No more having to awkwardly stretch up to type out numbers!",
    "version": "0.0.1",
    "publisher": "hesto2",
    "engines": {
        "vscode": "^1.27.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension",
    "contributes": {
        "keybindings": [
            {
                "command": "vsnumpad.print0",
                "key": "ctrl+shift+space",
                "when": "editorTextFocus"
            },
            {
                "command": "vsnumpad.print1",
                "key": "ctrl+shift+m",
                "when": "editorTextFocus"
            },
            {
                "command": "vsnumpad.print2",
                "key": "ctrl+shift+,",
                "when": "editorTextFocus"
            },
            {
                "command": "vsnumpad.print3",
                "key": "ctrl+shift+.",
                "when": "editorTextFocus"
            },
            {
                "command": "vsnumpad.print4",
                "key": "ctrl+shift+j",
                "when": "editorTextFocus"
            },
            {
                "command": "vsnumpad.print5",
                "key": "ctrl+shift+k",
                "when": "editorTextFocus"
            },
            {
                "command": "vsnumpad.print6",
                "key": "ctrl+shift+l",
                "when": "editorTextFocus"
            },
            {
                "command": "vsnumpad.print7",
                "key": "ctrl+shift+u",
                "when": "editorTextFocus"
            },
            {
                "command": "vsnumpad.print8",
                "key": "ctrl+shift+i",
                "when": "editorTextFocus"
            },
            {
                "command": "vsnumpad.print9",
                "key": "ctrl+shift+o",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.21",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    },
    "repository": {
        "url": "https://github.com/hesto2/vsnumpad"
    }
}
